---
apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: etx-quickstarts
  namespace: openshift-policy
  annotations:
    argocd.argoproj.io/sync-options: Validate=false
  labels:
      component: bootstrap
      purpose: etx-quickstarts
spec:
  project: default
  source:
    repoURL: https://github.com/redhat-ai-services/etx-agentic-ai.git
    targetRevision: main
    path: infra/quickstarts
  destination:
    server: https://kubernetes.default.svc
  syncPolicy:
    automated: {}
